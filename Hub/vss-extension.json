{
    "manifestVersion": 1,
    "id": "sample-extension",
    "version": "0.1.4",
    "name": "My first sample extension",
    "description": "A sample Visual Studio Services extension.",
    "publisher": "fabrikamdev",
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "contributions": [{
        "id": "Fabrikam.HelloWorld",
        "type": "ms.vss-web.hub",
        "description": "Adds a 'Hello' hub to the Work hub group.",
        "targets": [
            "ms.vss-work-web.work-hub-group"
        ],
        "properties": {
            "name": "Metrics",
            "order": 99,
            "uri": "deliveryMetrics.html"
        }
    }],
    "scopes": [
        "vso.work"
    ],
    "files": [{
            "path": "deliveryMetrics.html",
            "addressable": true
        },

        {
            "path": "sdk/scripts",
            "addressable": true
        },
        {
            "path": "js",
            "addressable": true
        }
    ]
}