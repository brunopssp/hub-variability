{
    "manifestVersion": 1,
    "id": "variability",
    "version": "0.2.27",
    "name": "Delivery Variability",
    "description": "This chart shows how long each item took to be done",
    "publisher": "TJSP",
    "icons": {
        "default": "img/graph.png"
    },
    "categories": [
        "Plan and track"
    ],
    "tags": [
        "working",
        "lead time",
        "cycle time",
        "throughput",
        "agile",
        "kanban",
        "lean"
    ],
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "contributions": [{
        "id": "variability",
        "type": "ms.vss-web.hub",
        "description": "This chart shows how long each item took to be done",
        "targets": [
            "ms.vss-work-web.work-hub-group"
        ],
        "properties": {
            "name": "Metrics",
            "order": 99,
            "uri": "deliveryMetrics.html"
        }
    }],
    "scopes": [
        "vso.work"
    ],
    "files": [{
            "path": "deliveryMetrics.html",
            "addressable": true
        },
        {
            "path": "sdk/scripts",
            "addressable": true
        },
        {
            "path": "img",
            "addressable": true
        }
    ]
}